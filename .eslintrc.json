{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "jest": true
  },
  "extends": ["airbnb-base"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "implicit-arrow-linebreak": 0,
    "function-paren-newline": 0,
    "arrow-parens": 0,
    "global-require": 0,
    "no-console": 0,
    "nonblock-statement-body-position": 0,
    "curly": 0,
    "consistent-return": 0
  }
}
